  "definitions": {
    "arista.subscriptions.Operation": {
      "type": "string",
      "enum": [
        "UNSPECIFIED",
        "INITIAL",
        "INITIAL_SYNC_COMPLETE",
        "UPDATED",
        "DELETED"
      ],
      "default": "UNSPECIFIED",
      "description": " - INITIAL: INITIAL indicates the associated notification is that of the\ncurrent state and a fully-specified Resource is provided.\n - INITIAL_SYNC_COMPLETE: INITIAL_SYNC_COMPLETE indicates all existing-state has been\nstreamed to the client. This status will be sent in an\notherwise-empty message and no subsequent INITIAL messages\nshould be expected.\n - UPDATED: UPDATED indicates the associated notification carries\nmodification to the last-streamed state. This indicates\nthe contained Resource may be a partial diff, though, it\nmay contain a fully-specified Resource.\n - DELETED: DETLETED indicates the associated notification carries\na deletion. The Resource's key will always be set in this case,\nbut no other fields should be expected."
    },
    "arista.syslog.v1.Export": {
      "type": "object",
      "properties": {
        "key": {
          "$ref": "#/definitions/arista.syslog.v1.ExportKey",
          "title": "key uniquely identifies a syslog server"
        },
        "stats": {
          "$ref": "#/definitions/arista.syslog.v1.ExportStats",
          "description": "stats contains the statistics for all export operations."
        },
        "lastError": {
          "$ref": "#/definitions/arista.syslog.v1.ExportError",
          "description": "last_error provides the details of the last export error."
        },
        "lastSuccess": {
          "$ref": "#/definitions/arista.syslog.v1.ExportSuccess",
          "description": "last_success provides details of last successful export."
        }
      },
      "description": "Export provides the export status for a syslog server."
    },
    "arista.syslog.v1.ExportConfig": {
      "type": "object",
      "properties": {
        "key": {
          "$ref": "#/definitions/arista.syslog.v1.ExportKey",
          "description": "key uniquely identifies a syslog server."
        }
      },
      "description": "ExportConfig is used to configure a syslog server."
    },
    "arista.syslog.v1.ExportConfigDeleteAllResponse": {
      "type": "object",
      "properties": {
        "type": {
          "$ref": "#/definitions/fmp.DeleteError",
          "description": "This describes the class of delete error."
        },
        "error": {
          "type": "string",
          "description": "This indicates the error message from the delete failure."
        },
        "key": {
          "$ref": "#/definitions/arista.syslog.v1.ExportKey",
          "description": "This is the key of the ExportConfig instance that failed to be deleted."
        },
        "time": {
          "type": "string",
          "format": "date-time",
          "description": "Time indicates the (UTC) timestamp when the key was being deleted."
        }
      }
    },
    "arista.syslog.v1.ExportConfigDeleteResponse": {
      "type": "object",
      "properties": {
        "key": {
          "$ref": "#/definitions/arista.syslog.v1.ExportKey",
          "description": "Key echoes back the key of the deleted ExportConfig instance."
        },
        "time": {
          "type": "string",
          "format": "date-time",
          "description": "- it is after the time the request was received\n   - a time-ranged query with StartTime==DeletedAt will not include this instance.",
          "title": "Time indicates the (UTC) timestamp at which the system recognizes the\ndeletion. The only guarantees made about this timestamp are:"
        }
      }
    },
    "arista.syslog.v1.ExportConfigResponse": {
      "type": "object",
      "properties": {
        "value": {
          "$ref": "#/definitions/arista.syslog.v1.ExportConfig",
          "description": "Value is the value requested.\nThis structure will be fully-populated as it exists in the datastore. If\noptional fields were not given at creation, these fields will be empty or\nset to default values."
        },
        "time": {
          "type": "string",
          "format": "date-time",
          "description": "Time carries the (UTC) timestamp of the last-modification of the\nExportConfig instance in this response."
        }
      }
    },
    "arista.syslog.v1.ExportConfigSetResponse": {
      "type": "object",
      "properties": {
        "value": {
          "$ref": "#/definitions/arista.syslog.v1.ExportConfig",
          "description": "Value carries all the values given in the ExportConfigSetRequest as well\nas any server-generated values."
        },
        "time": {
          "type": "string",
